class Solution {
public:
    bool judgeCircle(string m) {
        int l=0;
        int u=0;
        int r=0;
        int d=0;
      for(int i=0;i<m.size();i++){
            if(m[i]=='L'){
                l++;
            }
            else if(m[i]=='R'){
                l--;
            }
            else if(m[i]=='U'){
                u++;
            }
            else if(m[i]=='D'){
                u--;
            }
        }
        cout<<l;
   return l==0 && u==0;}
};
